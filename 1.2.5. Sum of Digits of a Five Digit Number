#include <stdio.h>

 int main() {
	 int n, sum = 0, digit;

	 //promt user to enter a 5-digit number
	 //printf("Enter a five-digit number: ");
	 scanf("%d", &n); //Read the number from the user

	 //Cheack if the number is exactly 5 digits
	 if (n >= 10000 && n <= 99999) {
		 //Extract each digit and add to sum
		 while (n > 0) {
			 digit = n % 10;  //Get last digit
			 sum += digit;   //Add it to sum
			 n /=10;         // Remove last digit from number
		 }
		 printf("%d\n", sum); // Print the sum of digits
	 } else {
		 //If number is not 5 digit, print invalid message
		 printf("Invalid input\n");
	 }
	 return 0; //End of program
 }
